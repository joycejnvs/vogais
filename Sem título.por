programa {
  funcao inicio() {
    caracter vogal[5]
    inteiro a=0 , e=0 , i=0 , o=0 , u=0 
    para(inteiro cont=0; cont<10; cont++){
    escreva("digite a vogal: ")
    leia(vogal[cont])
    }
    para(inteiro cont=0;cont<5;i++){

    se(vogal[cont]=="a" ou "A")
    a=(a++)

    se( vogal[cont]=="e" ou "E")
    e=(e++)

    se(vogal[cont]== "i" ou "I")
    i=(i++)

    se(vogal[cont]== "o" ou "O")
    o=(o++)

    se(vogal[cont]== "u" ou "U")
    u=(u++)







    }

  }
}
